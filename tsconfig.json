{
  "compilerOptions": {
    "moduleResolution": "Bundler",
    "module": "ESNext",
    "strictNullChecks": true,

    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "extends": "expo/tsconfig.base"
}
